<?xml version="1.0" encoding="utf-8" ?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">
  <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
  <style name="AppBaseTheme" parent="@android:style/Theme.Holo.Light">
    <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
  </style>

  <!-- Application theme. -->
  <style name="AppTheme" parent="@style/AppBaseTheme">
    <item name="vpiCirclePageIndicatorStyle">@style/AmwCirclePageIndicator</item>
    <item name="vpiTitlePageIndicatorStyle">@style/AmwTitlePageIndicator</item>
    <item name="vpiTabPageIndicatorStyle">@style/AmwTabPageIndicator</item>
    <item name="vpiTabTextStyle">@style/AmwTabPageIndicator.Text</item>
    <item name="android:actionBarStyle">@style/AmwActionBar</item>
  </style>
</resources>